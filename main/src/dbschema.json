{
	"db_name": "zms",
	"db_file": "data/zms.db",
	"version": 2,
	"maps": {
		"data/(users|authors)/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		"data/users/.+/data.json": {
			"to_table": []
		},
		"data/authors/.+/data.json": {
			"to_table": ["posts"]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["site", "TEXT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
			"schema_changed": 4
		},
		"posts": {
			"cols": [
				["id", "INTEGER"],
				["title", "TEXT"],
				["content", "TEXT"],
				["cut", "TEXT"],
				["json_id", "INTEGER"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
			"schema_changed": 4
		}
	}
}